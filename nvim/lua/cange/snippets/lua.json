{
  "pretty_print(…)": {
    "prefix": "pre",
    "body": "vim.pretty_print($1)"
  },
  "print(…)": {
    "prefix": "pri",
    "body": "print('--- $1:', $1)"
  },
  "notify(…)": {
    "prefix": "not",
    "body": "vim.notify('--- $1')"
  },
  "pcall(…)": {
    "prefix": "pca",
    "body": [
      "local found_$2, $2 = pcall(require, '$1')",
      "if not found_$2 then",
      "  print('${3:[${TM_FILENAME}]} \"$1\" not found')",
      "  return",
      "end",
      "$4"
    ]
  }
}
