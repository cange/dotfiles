{
  "pretty_print(…)": {
    "prefix": "pre",
    "body": "vim.pretty_print($1)"
  },
  "print(…)": {
    "prefix": "pri",
    "body": "print('--- $1:', $1)"
  },
  "notify(…)": {
    "prefix": "inf",
    "body": "vim.notify('--- $1')"
  },
  "pcall(…)": {
    "prefix": "pcall",
    "body": [
      "local found_$2, $2 = pcall(require, '$1')",
      "if not found_$2 then",
      "  vim.notify('${3:context}: \"$1\" could not be found')",
      "  return",
      "end",
      "$4"
    ]
  }
}
