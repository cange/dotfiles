{
  "pretty_print(…)": {
    "prefix": "pre",
    "body": "vim.pretty_print($1)"
  },
  "pretty_print('…',…)": {
    "prefix": "prel",
    "body": "vim.pretty_print('$1', $1)"
  },
  "print(…)": {
    "prefix": "pri",
    "body": "print('--- $1:', $1)"
  },
  "notify(…)": {
    "prefix": "not",
    "body": "vim.notify('--- $1')"
  },
  "local func(…)": {
    "prefix": "lf",
    "body": "local function $1($2)\n\t$0\nend"
  },
  "tbl_keys(…)": {
    "prefix": "tblk",
    "body": "vim.tbl_${1|extend,keys,deep_extend|}($2)"
  },
  "pcall(…)": {
    "prefix": "pca",
    "body": [
      "local found_$2, $2 = pcall(require, '$1')",
      "if not found_$2 then",
      "  print('${3:[${TM_FILENAME}]} \"$1\" not found')",
      "  return",
      "end",
      "$4"
    ]
  }
}
