{
  "prettier-ignore": {
    "prefix": "prettier",
    "description": "exclude the next node",
    "body": "<!-- prettier-ignore -->"
  },
  "prettier-ignore-attribute": {
    "prefix": "lintprattr",
    "body": "<!-- prettier-ignore-attribute -->"
  },
  "prettier-ignore-attribute key": {
    "prefix": "lintprattrk",
    "body": "<!-- prettier-ignore-attribute ($keyword) -->"
  },
  "pre block debug": {
    "prefix": "ptag",
    "body": "<pre class=\"debug\">$1: {{ $1 }}</pre>\n"
  }
}
