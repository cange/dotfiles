{
  "eslint block": {
    "prefix": "eslint-disable … */",
    "body": [
      "/* eslint-disable $1 */",
      "$2",
      "/* eslint-enable $1 */"
    ]
  },
  "disable prettier block": {
    "prefix": "eslint prettier/prettier */",
    "body": [
      "/* eslint-disable prettier/prettier */",
      "$2",
      "/* eslint-enable prettier/prettier */"
    ]
  },
  "eslint-disable-line": {
    "prefix": "eslint-disable-line …",
    "body": "// eslint-disable-line $1"
  },
  "if": {
    "prefix": "if (…",
    "body": "if ($1) {\n\t$2\n}"
  },
  "forEach": {
    "prefix": ".forEach(…",
    "body": ".forEach($1 => $2)"
  },
  "map": {
    "prefix": ".map(…",
    "body": ".map($1 => $2)"
  },
  "filter": {
    "prefix": ".filter(…",
    "body": ".filter($1 => $2)"
  },
  "console.log(": {
    "prefix": "log",
    "body": "console.log('--- ${1:param}: %o', $1)"
  },
  "log(Object.keys(…": {
    "prefix": "logkeys",
    "body": "console.log('--- ${1:param} keys: %o', Object.keys($1))"
  },
  "console.table(": {
    "prefix": "logtable",
    "body": "console.table($1)"
  },
  "import local": {
    "prefix": "import local",
    "body": "import ${1:componentName} from './$1'"
  },
  "querySelector": {
    "prefix": ".querySelector(…",
    "body": ".querySelector('${1:selector}')$2"
  },
  "querySelectorAll": {
    "prefix": ".querySelectorAll(…",
    "body": ".querySelectorAll('${1:selector}')$2"
  },
  "DOM ready": {
    "prefix": "ready",
    "body": "document.addEventListener('DOMContentLoaded', () => {\n\t$1\n})"
  },
  "addEventListener": {
    "prefix": ".addEventListener(…",
    "body": ".addEventListener('${1:eventName}', ${2:event} => {\n\t$3\n})"
  },
  "createElement": {
    "prefix": ".createElement",
    "body": ".createElement('${1:element}')$2"
  },
  "classList.add": {
    "prefix": ".classList.add(…",
    "body": ".classList.add('${1:class}')"
  },
  "classList.remove": {
    "prefix": ".classList.remove(…",
    "body": ".classList.remove('${1:class}')"
  },
  "classList.toggle": {
    "prefix": ".classList.toggle(…",
    "body": ".classList.toggle('${1:class}')"
  },
  "getAttribute": {
    "prefix": ".getAttribute(…",
    "body": ".getAttribute('${1:attr}')"
  },
  "removeAttribute": {
    "prefix": ".removeAttribute(…",
    "body": ".removeAttribute('${1:attr}')$2"
  },
  "setAttribute": {
    "prefix": ".setAttribute(…, …",
    "body": ".setAttribute('${1:attr}', ${2:value})$3"
  },
  "Object.keys": {
    "prefix": "Object.keys(…",
    "body": "Object.keys(${1:obj})$2"
  },
  "Object.values": {
    "prefix": "Object.values(…",
    "body": "Object.values(${1:obj})$2"
  }
}
