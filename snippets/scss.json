{
  "v-breakpoint(…) - mixin": {
    "prefix": "brem",
    "body": "@include v-breakpoint(${1|desktop,tablet,mobile|}, ${2|up,only,down|}) {\n\t$3\n}"
  },
  "v-breakpoint(…) - function": {
    "prefix": "bref",
    "body": "v-breakpoint(${1|desktop,tablet,mobile|}, ${2|min,max|})"
  },
  "@use - design-tokens": {
    "prefix": "@usedt",
    "body": "@$use '@visable-dev/design-tokens/dist/tokens' as *;\n$2"
  },
  "@group - SassDoc": {
    "prefix": "@group",
    "body": "////\n/// @group $1\n////"
  },
  "@param - SassDoc": {
    "prefix": "@param",
    "body": "/// @param {$1} $2${3:- }"
  },
  "@returns - SassDoc": {
    "prefix": "@returns",
    "body": "/// @returns $1${3:- }"
  },
  "@deprecated - SassDoc": {
    "prefix": "@deprecated",
    "body": "/// @deprecated\n/// @since <unreleased>"
  },
  "@since - SassDoc": {
    "prefix": "@since",
    "body": "/// @since <unreleased>"
  },
  "map.get(…)": {
    "prefix": "mapg",
    "body": "map.get($${1:props}, ${2:key})"
  },
  "map.merge(…)": {
    "prefix": "mapm",
    "body": "map.merge($${1:props}, $2)"
  }
}
